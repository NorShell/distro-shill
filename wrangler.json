{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "distro-shill",
  "main": "src/index.ts",
  "compatibility_date": "2025-01-24",
  "observability": {
    "enabled": false
  },
  "assets": {
    "directory": "./public/",
    "binding": "ASSETS"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "Distro",
      "database_id": "e8a24658-7ffe-45bb-a5dd-7728f0188c90"
    }
  ],
  "r2_buckets": [
    {
      "binding": "BUCKET",
      "bucket_name": "distros"
    }
  ]
}
